<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10.0" prefHeight="450.0" prefWidth="600.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.peerbox.presenter.settings.synchronization.Synchronization">
	<columnConstraints>
		<ColumnConstraints hgrow="ALWAYS" minWidth="60.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints />
		<RowConstraints minHeight="-Infinity" prefHeight="150.0" vgrow="SOMETIMES" />
		<RowConstraints />
	</rowConstraints>
	<children>
		 <VBox spacing="10.0" GridPane.rowIndex="0">
			<children>
				<Label text="Synchronization">
					<font>
						<Font name="System Bold" size="13.0" />
					</font>
					<VBox.margin>
						<Insets />
					</VBox.margin>
				</Label>
				<Label text="Only marked files are synchronized to this device. Changes are only applied if you push &quot;OK.&quot;" wrapText="true" />
			</children>
		</VBox>
		<GridPane hgap="10.0" vgap="10.0" GridPane.rowIndex="1">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="50.0" />
				<ColumnConstraints hgrow="NEVER" minWidth="20.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<VBox GridPane.columnIndex="0" >
					<children>
           				<TreeView fx:id="fileTreeView" layoutX="12.0" layoutY="158.0" prefHeight="193.0" prefWidth="471.0" />
           			</children>
           		</VBox>
  
			</children>
		</GridPane>
		<HBox alignment="CENTER_LEFT" GridPane.rowIndex="2">
			<children>
				<Button defaultButton="true" onAction="#acceptSyncAction" text="_OK" />
				<Button cancelButton="true" onAction="#cancelAction" text="_Cancel">
					<HBox.margin>
						<Insets left="10.0" />
					</HBox.margin>
				</Button>
			</children>
			<padding>
				<Insets top="20.0" />
			</padding>
		</HBox>
	</children>
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
</GridPane>
