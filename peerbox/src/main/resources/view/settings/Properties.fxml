<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10.0" minHeight="200.0" minWidth="200.0" prefHeight="300.0" prefWidth="250.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.peerbox.presenter.settings.synchronization.Synchronization">
	<columnConstraints>
		<ColumnConstraints hgrow="ALWAYS" minWidth="60.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints maxHeight="60.0" prefHeight="30.0" vgrow="NEVER" />
		<RowConstraints prefHeight="150.0" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="NEVER" />
	</rowConstraints>
	<children>
		 <VBox minHeight="16.0" prefHeight="16.0" prefWidth="230.0" spacing="10.0" GridPane.rowIndex="0" />
		<HBox alignment="CENTER_LEFT" GridPane.rowIndex="2">
			<children>
				<Button fx:id="okButton" defaultButton="true" onAction="#acceptSyncAction" text="Ok" />
			</children>
			<padding>
				<Insets top="20.0" />
			</padding>
		</HBox>
      <GridPane GridPane.rowIndex="1">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints fillHeight="false" minHeight="30.0" prefHeight="30.0" vgrow="NEVER" />
          <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ListView prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1" />
            <Label text="This folder is shared between">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </GridPane>
	</children>
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
</GridPane>
